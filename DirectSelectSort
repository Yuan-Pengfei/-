public static void DirectInsertSort(int[] a){
    int i,j;
    for(i=1;i<a.length;i++){
        int temp=a[i];
        for(j=i-1;j>=0;j--){
            if(a[j]>temp){
                a[j+1]=a[j];
            }
            else{
                break;
            }
        }
        a[j+1]=temp;
    }
}
